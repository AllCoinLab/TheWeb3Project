pragma solidity ^0.8.2;

/////////////////////////////////////////////////////////////////////////////////////////////////////// GetEnum
library GetEnum { //////////////////////////////////////////////////////////// storage?
  using EnumerableSet for EnumerableSet.AddressSet;
  function getEnumValues(EnumerableSet.AddressSet storage data, address adr, uint idx, uint n) internal view returns (bool, uint, address, address[] memory) {
    if (n == 0) {
      return (data.contains(adr), data.length(), data.at(idx), data.values()); // big cost
    } else {
      if (data.length() < n) {
        n = data.length();
      }

      if (n == 0) { // no data
        address[] memory nullValues = new address[](1);
        return (data.contains(adr), data.length(), nullValues[0], nullValues);
      }

      // data avail
      address[] memory values = new address[](n);
      
      for (uint idy = 0; idy < n; idy++) {
        values[idy] = data.at(idy);
      }
      return (data.contains(adr), data.length(), data.at(idx), values);
    }
  }
}


/////////////////////////////////////////////////////////////////////////////////////////////////////// Tx
library Tx {
    function safeApprove(address token, address to, uint value) internal {
        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(0x095ea7b3, to, value));
        require(success && (data.length == 0 || abi.decode(data, (bool))), 'Tx: APPROVE_FAILED');
    }

    function safeTransfer(address token, address to, uint value) internal {
        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(0xa9059cbb, to, value));
        require(success && (data.length == 0 || abi.decode(data, (bool))), 'Tx: TRANSFER_FAILED');
    }

    function safeTransferFrom(address token, address from, address to, uint value) internal {
        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(0x23b872dd, from, to, value));
        require(success && (data.length == 0 || abi.decode(data, (bool))), 'Tx: TRANSFER_FROM_FAILED');
    }
    
    // sends ETH or an erc20 token
    function safeTransferToken(address token, address to, uint value, bool isEth) internal {
        if (isEth) {
            (bool v,) = to.call{ value: value }(new bytes(0));
            require(v, "Transfer Failed");
        } else {
            safeTransfer(token, to, value);
        }
    }
}






